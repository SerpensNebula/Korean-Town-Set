                spritelayout layout_PLAYGROUND_APT_2000 {
                    ground {
                        sprite:          GROUNDSPRITE_CONCRETE;
                    }

                    building {
                        sprite:          set_PLAYGROUND_APT_2000;
                        recolour_mode:   RECOLOUR_REMAP;
                        palette:         PALETTE_USE_DEFAULT;
                    }
                }

                spriteset (set_PLAYGROUND_APT_2000, "./Korean_Town_Set/gfx/PLAYGROUND.png") {
                    tmpl_low_building(0, 0)
                }

            // Item Definition
                item(FEAT_HOUSES, kt_PLAYGROUND_APT_2000, 164, HOUSE_SIZE_1X1) {
                    property {
                        substitute:                     0; // 1x1
                        name:                           string(STR_PLAYGROUND_APT_NAME);
                        availability_mask:              [ALL_TOWNZONES, ALL_CLIMATES | bitmask(ABOVE_SNOWLINE)];
                        probability:                    2;
                        years_available:                [2000, 65535];
                        minimum_lifetime:               4*(para_lifetime);

                        population:                     0;
                        mail_multiplier:                0;
                        accepted_cargos:                [[PASS, 4], [MAIL, 0], [GOOD, 0]];
                    
                        local_authority_impact:         35;
                        removal_cost_multiplier:        50;

                        building_class:                 10; // MID_APT_2000
                    }

                    graphics {
                        default:                        layout_PLAYGROUND_APT_2000;
                        colour:                         random_bits%15+775;
                        construction_check:             sw_check_avoid_APT;
                    }
                }

